{
  "name": "vnoi-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 3",
    "lint:fix": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 3 --fix",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "vitest run && pnpm cypress:component",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:component": "pnpm cypress:component:chrome && pnpm cypress:component:firefox",
    "cypress:component:chrome": "cypress run --component --browser chrome",
    "cypress:component:firefox": "cypress run --component --browser firefox",
    "cypress:e2e": "pnpm cypress:e2e:chrome && pnpm cypress:e2e:firefox",
    "cypress:e2e:chrome": "cypress run --browser chrome",
    "cypress:e2e:firefox": "cypress run --browser firefox"
  },
  "dependencies": {
    "@codemirror/lang-cpp": "^6.0.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/joy": "^5.0.0-beta.8",
    "@mui/lab": "^5.0.0-alpha.140",
    "@mui/material": "^5.15.12",
    "@mui/system": "^5.15.12",
    "@mui/x-date-pickers": "^6.11.2",
    "@mui/x-tree-view": "^7.3.1",
    "@uiw/codemirror-theme-okaidia": "^4.21.9",
    "@uiw/react-codemirror": "^4.21.9",
    "@vidstack/react": "^1.11.21",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "file-saver": "^2.0.5",
    "firebase": "^10.4.0",
    "hls.js": "^1.5.8",
    "joi": "^17.10.2",
    "jszip": "^3.10.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-joyride": "^2.5.5",
    "react-markdown": "^8.0.7",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.15.0",
    "react-split": "^2.0.14",
    "react-toastify": "^9.1.3",
    "remark-gfm": "^3.0.1",
    "universal-cookie": "^6.1.1",
    "use-debounce": "^10.0.0",
    "vitest": "^1.6.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^1.6.0",
    "cypress": "^13.12.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.3.2",
    "vite": "^4.4.5"
